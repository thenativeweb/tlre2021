{
  "plugins": [
    "react-hooks",
    "styled-components-a11y",
    "jest"
  ],
  "extends": [
    "es/browser",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended",
    "plugin:styled-components-a11y/recommended"
  ],
  "env":{
    "jest/globals": true
  },
  "rules": {
    "styled-components-a11y/no-onchange": 0
  },
  "ignorePatterns": ["!**/*", ".storybook/*", "./src/stories/**"]
}
